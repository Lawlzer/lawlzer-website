{
  "name": "lawlzer-site",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "keywords": [],
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/esm.js",
      "require": "./dist/cjs.js"
    },
    "./package.json": "./package.json",
    "./*": {
      "source": "./src/*.ts",
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "main": "./dist/cjs.js",
  "module": "./dist/esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "npx --yes prisma generate && next build",
    "build:local": "cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "build:run": "next build && next start -p ${PORT:-3000}",
    "check": "next lint && tsc --noEmit",
    "clean": "rimraf ./dist",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "debug:reset-data-platform": "tsx scripts/debugResetDataPlatform.ts",
    "dev": "nodemon --watch src --ext ts,tsx,js,jsx,mdx --exec \"next dev --turbo\"",
    "dev:simple": "next dev",
    "dev:watch": "nodemon --watch src --ext ts,tsx,js,jsx,mdx --exec \"next dev --turbo\"",
    "lint": "next lint",
    "lint:all": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --config .config/.eslint.config.mjs --fix",
    "lint:eslint:commit": "cross-env LINT_STAGED_COMMIT=true eslint --config .config/.eslint.config.mjs --fix",
    "lint:eslint:debug": "cross-env TIMING=1 eslint --config .config/.eslint.config.mjs",
    "lint:prettier": "prettier --write --cache \"**/*.{ts,tsx,js,jsx,mdx}\"",
    "lint:prettier:commit": "cross-env LINT_STAGED_COMMIT=true prettier --write --cache \"**/*.{ts,tsx,js,jsx,mdx}\"",
    "lint:prettier:debug": "prettier --write --ignore-path ./.gitignore",
    "lint:staged": "lint-staged",
    "pre-commit": "lint-staged",
    "prepare": "node scripts/prepare.cjs",
    "preview": "next build && next start",
    "publish:all": "npm run build && npm run test && npm publish --access=public && echo \"Successfully published to NPM!\"",
    "start": "next start",
    "test": "echo \"Tests skipped\"",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:watch": "nodemon --watch app --watch src --watch tests --ext ts,tsx,js,jsx,mdx --exec \"cross-env SKIP_ENV_VALIDATION=true playwright test\"",
    "test:unit": "cross-env SKIP_ENV_VALIDATION=true vitest run",
    "test:unit:watch": "cross-env SKIP_ENV_VALIDATION=true vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mdx}": [
      "eslint --config .config/.eslint.config.mjs --fix",
      "prettier --write"
    ]
  },
  "prettier": "./.prettierrc.mjs",
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@lawlzer/utils": "^4.0.2",
    "@prisma/client": "^6.10.1",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query": "^5.81.0",
    "@tanstack/react-query-devtools": "^5.81.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@trpc/client": "^11.4.2",
    "@trpc/react-query": "^11.4.2",
    "@trpc/server": "^11.4.2",
    "@types/recharts": "^1.8.29",
    "@visx/visx": "^3.12.0",
    "abortcontroller-polyfill": "^1.7.8",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.18.1",
    "fs-extra": "^11.3.0",
    "jsonc-parser": "^3.3.1",
    "mongodb": "^6.17.0",
    "next": "^14.2.8",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-speech-kit": "^3.0.1",
    "react-zoom-pan-pinch": "^3.7.0",
    "react-zxing": "^2.1.0",
    "recharts": "^2.15.3",
    "server-only": "^0.0.1",
    "sharp": "^0.34.2",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.53.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^15.0.0",
    "@types/bun": "^1.2.16",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.11.24",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^3.2.4",
    "axe-playwright": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.8",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "next-test-api-route-handler": "^4.0.16",
    "nodemon": "^3.1.10",
    "playwright": "^1.53.1",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "prisma": "^6.10.1",
    "rimraf": "^6.0.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4",
    "typescript-eslint": "7.18.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "npm@11.4.2",
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  }
}
