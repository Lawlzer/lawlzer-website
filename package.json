{
	"name": "lawlzer-site",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "next lint && tsc --noEmit",
		"db:generate": "prisma migrate dev",
		"db:migrate": "prisma migrate deploy",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"dev": "next dev --turbo",
		"postinstall": "prisma generate",
		"lint": "echo Linting all files: && npm run lint:all",
		"lint:all": "npm run lint:eslint && npm run lint:prettier",
		"lint:eslint": "eslint --fix --config ./.config/.eslint.config.mjs",
		"lint:prettier": "prettier --write --cache --config ./.config/.prettierrc.mjs \"**/*.{ts,tsx,js,jsx,mdx}\"",
		"lint:staged": "lint-staged",
		"prepare": "husky",
		"preview": "next build && next start",
		"start": "next start",
		"typecheck": "tsc --noEmit"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,mdx}": [
			"eslint --config ./.config/.eslint.config.mjs --fix",
			"prettier --config ./.config/.prettierrc.mjs --write"
		]
	},
	"prettier": "./.config/.prettierrc.mjs",
	"dependencies": {
		"@auth/prisma-adapter": "^2.7.2",
		"@lawlzer/utils": "^3.4.15",
		"@prisma/client": "^6.5.0",
		"@t3-oss/env-nextjs": "^0.12.0",
		"@tanstack/query-core": "^5.72.0",
		"@tanstack/react-query": "^5.69.0",
		"@trpc/client": "^11.0.0",
		"@trpc/react-query": "^11.0.0",
		"@trpc/server": "^11.0.0",
		"fs-extra": "^11.3.0",
		"next": "^15.2.3",
		"next-auth": "5.0.0-beta.25",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"server-only": "^0.0.1",
		"superjson": "^2.2.1",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@tailwindcss/postcss": "^4.0.15",
		"@types/node": "^22.14.0",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"@typescript-eslint/parser": "^8.29.1",
		"eslint": "^9.23.0",
		"eslint-config-next": "^15.2.3",
		"eslint-plugin-import": "^2.31.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.0",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-packagejson": "^2.5.10",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"prisma": "^6.5.0",
		"tailwindcss": "^4.0.15",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.29.1"
	},
	"packageManager": "npm@11.1.0",
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	}
}
