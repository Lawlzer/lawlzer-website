{
	"name": "lawlzer-site",
	"version": "0.1.0",
	"private": true,
	"description": "",
	"keywords": [],
	"license": "ISC",
	"type": "module",
	"exports": {
		".": {
			"source": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/esm.js",
			"require": "./dist/cjs.js"
		},
		"./package.json": "./package.json",
		"./*": {
			"source": "./src/*.ts",
			"types": "./dist/*.d.ts",
			"import": "./dist/*.js",
			"require": "./dist/*.cjs"
		}
	},
	"main": "./dist/cjs.js",
	"module": "./dist/esm.js",
	"types": "./dist/index.d.ts",
	"files": [
		"src",
		"dist"
	],
	"scripts": {
		"build": "next build",
		"check": "next lint && tsc --noEmit",
		"clean": "rimraf ./dist",
		"db:generate": "prisma migrate dev",
		"db:migrate": "prisma migrate deploy",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"debug:reset-data-platform": "tsx scripts/debugResetDataPlatform.ts",
		"dev": "next dev --turbo",
		"lint": "lint-staged",
		"lint:all": "npm run lint:eslint && npm run lint:prettier",
		"lint:eslint": "eslint --fix --config ./.config/.eslint.config.mjs",
		"lint:eslint:commit": "cross-env LINT_STAGED_COMMIT=true eslint --fix --config ./.config/.eslint.config.mjs",
		"lint:eslint:debug": "cross-env TIMING=1 eslint --config ./.config/.eslint.config.mjs",
		"lint:prettier": "prettier --write --cache --config ./.config/.prettierrc.mjs \"**/*.{ts,tsx,js,jsx,mdx}\"",
		"lint:prettier:commit": "cross-env LINT_STAGED_COMMIT=true prettier --write --cache --config ./.config/.prettierrc.mjs \"**/*.{ts,tsx,js,jsx,mdx}\"",
		"lint:prettier:debug": "prettier --write --config ./.config/.prettierrc.mjs --ignore-path ./.gitignore",
		"lint:staged": "lint-staged",
		"pre-commit": "lint-staged",
		"prepare": "node -e \"try { require.resolve('husky'); require('child_process').execSync('husky', {stdio: 'inherit'}); } catch (e) {}\"",
		"preview": "next build && next start",
		"start": "next start",
		"test": "npm run test:unit && npm run test:e2e",
		"test:all": "npm run test:unit && npm run test:e2e",
		"test:e2e": "cross-env SKIP_ENV_VALIDATION=true playwright test",
		"test:e2e:watch": "nodemon --watch app --watch src --watch tests --ext ts,tsx,js,jsx,mdx --exec \"cross-env SKIP_ENV_VALIDATION=true playwright test\"",
		"test:unit": "cross-env SKIP_ENV_VALIDATION=true vitest run",
		"test:unit:watch": "cross-env SKIP_ENV_VALIDATION=true vitest watch",
		"typecheck": "tsc --noEmit"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,mdx}": [
			"eslint --config ./.config/.eslint.config.mjs --fix",
			"prettier --config ./.config/.prettierrc.mjs --write"
		]
	},
	"prettier": "./.config/.prettierrc.mjs",
	"dependencies": {
		"@headlessui/react": "^2.2.4",
		"@heroicons/react": "^2.2.0",
		"@lawlzer/utils": "^4.0.2",
		"@prisma/client": "^6.9.0",
		"@t3-oss/env-nextjs": "^0.13.8",
		"@tailwindcss/postcss": "^4.1.10",
		"@tanstack/react-query": "^5.80.7",
		"@trpc/client": "^11.4.1",
		"@trpc/react-query": "^11.4.1",
		"@trpc/server": "^11.4.1",
		"@visx/visx": "3.13.2-alpha.0",
		"autoprefixer": "^10.4.21",
		"clsx": "^2.1.1",
		"critters": "^0.0.25",
		"date-fns": "^4.1.0",
		"framer-motion": "^12.18.1",
		"fs-extra": "^11.3.0",
		"jsonc-parser": "^3.3.1",
		"mongodb": "^6.17.0",
		"next": "^15.3.3",
		"next-themes": "^0.4.6",
		"postcss": "^8.5.6",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-zoom-pan-pinch": "^3.7.0",
		"server-only": "^0.0.1",
		"sharp": "^0.34.2",
		"superjson": "^2.2.2",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.10",
		"zod": "^3.25.64"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@playwright/test": "^1.53.0",
		"@testing-library/dom": "^10.4.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/bun": "^1.2.16",
		"@types/node": "^24.0.3",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@typescript-eslint/parser": "^8.34.0",
		"@vitejs/plugin-react": "^4.5.2",
		"@vitest/coverage-v8": "^3.2.3",
		"cross-env": "^7.0.3",
		"dotenv": "^16.5.0",
		"eslint": "^9.29.0",
		"eslint-config-next": "^15.3.3",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"eslint-plugin-unused-imports": "^4.1.4",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"lint-staged": "^16.1.2",
		"next-test-api-route-handler": "^4.0.16",
		"nodemon": "^3.1.10",
		"playwright": "^1.53.0",
		"prettier": "^3.5.3",
		"prettier-plugin-packagejson": "^2.5.15",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"prisma": "^6.9.0",
		"rimraf": "^6.0.1",
		"tailwindcss-animate": "^1.0.7",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.0",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.3"
	},
	"packageManager": "npm@11.4.2"
}
