{
	"name": "lawlzer-site",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"build:run": "next build && next start",
		"check": "next lint && tsc --noEmit",
		"db:generate": "prisma migrate dev",
		"db:migrate": "prisma migrate deploy",
		"db:push": "prisma db push",
		"db:studio": "prisma studio",
		"dev": "nodemon --watch src --ext ts,tsx,js,jsx,mdx --exec \"next dev --turbo\"",
		"postinstall": "prisma generate",
		"lint": "echo Linting all files: && npm run lint:all",
		"lint:all": "npm run lint:eslint && npm run lint:prettier",
		"lint:eslint": "eslint --fix --config ./.config/.eslint.config.mjs",
		"lint:prettier": "prettier --write --cache --config ./.config/.prettierrc.mjs \"**/*.{ts,tsx,js,jsx,mdx}\"",
		"lint:staged": "lint-staged",
		"pre-commit": "lint-staged",
		"prepare": "husky",
		"preview": "next build && next start",
		"start": "next start",
		"test": "npm run test:unit && npm run test:e2e",
		"test:e2e": "cross-env SKIP_ENV_VALIDATION=true playwright test",
		"test:unit": "cross-env SKIP_ENV_VALIDATION=true vitest run",
		"test:watch": "cross-env SKIP_ENV_VALIDATION=true vitest watch && cross-env SKIP_ENV_VALIDATION=true playwright test --watch",
		"typecheck": "tsc --noEmit"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,mdx}": [
			"eslint --config ./.config/.eslint.config.mjs --fix",
			"prettier --config ./.config/.prettierrc.mjs --write"
		]
	},
	"prettier": "./.config/.prettierrc.mjs",
	"dependencies": {
		"@headlessui/react": "^2.2.1",
		"@heroicons/react": "^2.2.0",
		"@lawlzer/utils": "^3.4.15",
		"@prisma/client": "^6.6.0",
		"@t3-oss/env-nextjs": "^0.12.0",
		"@tanstack/query-core": "^5.73.3",
		"@tanstack/react-query": "^5.73.3",
		"@trpc/client": "^11.1.0",
		"@trpc/react-query": "^11.1.0",
		"@trpc/server": "^11.1.0",
		"@types/jsonwebtoken": "^9.0.9",
		"fs-extra": "^11.3.0",
		"jsdom": "^26.1.0",
		"jsonwebtoken": "^9.0.2",
		"lucide-react": "^0.488.0",
		"next": "^15.3.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-icons": "^5.5.0",
		"server-only": "^0.0.1",
		"superjson": "^2.2.2",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@babel/core": "^7.26.10",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-typescript": "^7.27.0",
		"@eslint/eslintrc": "^3.3.1",
		"@playwright/test": "^1.51.1",
		"@tailwindcss/postcss": "^4.1.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^22.14.1",
		"@types/react": "^19.1.1",
		"@types/react-dom": "^19.1.2",
		"@typescript-eslint/parser": "^8.29.1",
		"@vitest/coverage-v8": "^3.1.1",
		"cross-env": "^7.0.3",
		"dotenv": "^16.5.0",
		"eslint": "^9.24.0",
		"eslint-config-next": "^15.3.0",
		"eslint-plugin-import": "^2.31.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.5.1",
		"next-test-api-route-handler": "^4.0.16",
		"nodemon": "^3.1.9",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-packagejson": "^2.5.10",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"prisma": "^6.6.0",
		"tailwindcss": "^4.1.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.29.1",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.1.1"
	},
	"packageManager": "npm@11.3.0",
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	}
}
